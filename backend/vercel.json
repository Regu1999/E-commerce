        {
          "version": 2,
          "builds": [
            {
              "src": "api/app.js",
              "use": "@vercel/node"
            }
          ],
          "rewrites": [
            {
              "source": "/(.*)",
              "destination": "/api/app.js"
            }
          ],
          "routes": [
            {
              "src": "/images/(.*)",
              "dest": "/public/images/$1"
            },
            {
              "src": "/(.*)",
              "dest": "/api/app.js"
            }
          ]
        }